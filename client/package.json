{
  "name": "spice",
  "version": "0.2.0",
  "description": "Debug through time.",
  "scripts": {
    "start": "tsc && npm run sass && concurrently \"tsc -w\" \"lite-server\" \"npm run sass-watch\" ",
    "sass": "node node_modules/node-sass/bin/node-sass styles/spice.scss spice.css",
    "sass-watch": "node node_modules/node-sass/bin/node-sass --watch styles/spice.scss spice.css",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "build:aot": "ngc -p tsconfig.aot.json && rollup -c",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "pree2e": "webdriver-manager update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/flex-layout": "2.0.0-beta.6",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/material": "2.0.0-beta.3",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/platform-server": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@types/prismjs": "^1.6.1",
    "angular-in-memory-web-api": "~0.2.4",
    "core-js": "^2.4.1",
    "d3": "^4.6.0",
    "hammerjs": "^2.0.8",
    "normalize-scss": "^6.0.0",
    "oboe": "^2.1.3",
    "prismjs": "^1.6.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/d3": "^4.5.0",
    "@types/jasmine": "2.5.41",
    "@types/node": "^6.0.46",
    "@types/reflect-metadata": "^0.0.5",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "node-sass": "^4.2.0",
    "protractor": "~4.0.14",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "tslint": "^3.15.1",
    "typescript": "~2.2.2"
  },
  "repository": {}
}
